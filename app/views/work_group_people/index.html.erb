<p id="notice"><%= notice %></p>

<h1>Work Group People</h1>

<table>
  <thead>
    <tr>
      <th>Idworkgroup</th>
      <th>Documentnumberperson</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @work_group_people.each do |work_group_person| %>
      <tr>
        <td><%= work_group_person.idWorkGroup %></td>
        <td><%= work_group_person.documentNumberPerson %></td>
        <td><%= link_to 'Show', work_group_person %></td>
        <td><%= link_to 'Edit', edit_work_group_person_path(work_group_person) %></td>
        <td><%= link_to 'Destroy', work_group_person, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

