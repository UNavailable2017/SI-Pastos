<p id="notice"><%= notice %></p>

<h1>Censos</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Numberofchildren</th>
      <th>Originresguardo</th>
      <th>Levelstudies</th>
      <th>Profession</th>
      <th>Entry</th>
      <th>Person</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @censos.each do |censo| %>
      <tr>
        <td><%= censo.date %></td>
        <td><%= censo.numberOfChildren %></td>
        <td><%= censo.originResguardo %></td>
        <td><%= censo.levelStudies %></td>
        <td><%= censo.profession %></td>
        <td><%= censo.entry %></td>
        <td><%= censo.person_id %></td>
        <td><%= link_to 'Show', censo %></td>
        <td><%= link_to 'Edit', edit_censo_path(censo) %></td>
        <td><%= link_to 'Destroy', censo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Censo', new_censo_path %>
