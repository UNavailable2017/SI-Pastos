<% if user_signed_in? %>
  <% if current_user.try(:admin?) %>
    <!-- is admin -->
    <div class="container">
      <div class="row">
        <%= link_to "Inicio", root_path, class: "waves-effect waves-light btn col m3" %>
        <%= link_to "Mapa", residences_path, class: "waves-effect waves-light btn col m2" %>
        <%= link_to "Certificado", new_certificate_path, class: "waves-effect waves-light btn col m2" %>
        <%= link_to "Eventos", events_path, class: "waves-effect waves-light btn col m3" %>
        <%= link_to "Empleo", announcements_path, class: "waves-effect waves-light btn col m2" %>

      </div>
    </div>

  <% else %>

    <!-- is user -->
    <div class="container">
      <div class="row">
        <%= link_to "Inicio", root_path, class: "waves-effect waves-light btn col m4" %>
        <%= link_to "Eventos", events_path, class: "waves-effect waves-light btn col m4" %>
        <%= link_to "Empleo", announcements_path, class: "waves-effect waves-light btn col m4" %>
        <!-- <a class="waves-effect waves-teal btn-flat">Button</a>
        <a class="waves-effect waves-light btn col m4">Inicio</a>
        <a class="waves-effect waves-light btn col m4">Empleos</a>
        <a class="waves-effect waves-light btn col m4">Eventos</a> -->
      </div>
    </div>

  <% end %>

<% end %>
