<%= form_with(model: health_service, local: true) do |form| %>
  <% if health_service.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(health_service.errors.count, "error") %> prohibited this health_service from being saved:</h2>

      <ul>
      <% health_service.errors.full_messages.each do |message| %>
