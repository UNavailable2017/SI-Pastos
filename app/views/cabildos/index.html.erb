<p id="notice"><%= notice %></p>

<h1>Cabildos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Workgroup</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cabildos.each do |cabildo| %>
      <tr>
        <td><%= cabildo.name %></td>
        <td><%= cabildo.address %></td>
        <td><%= cabildo.phone %></td>
        <td><%= cabildo.workGroup %></td>
        <td><%= link_to 'Show', cabildo %></td>
        <td><%= link_to 'Edit', edit_cabildo_path(cabildo) %></td>
        <td><%= link_to 'Destroy', cabildo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cabildo', new_cabildo_path %>
